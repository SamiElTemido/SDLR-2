							</entries>
						</stimulus_data>
						<signals>
							<signal path="/BinaryToDecimal/RST"/>
						</signals>
					</stimulus>
					<stimulus name="Stimulus1" strength="default" enabled="true" type="formula">
						<stimulus_data>
							<repeat_time value="1500us" enabled="true"/>
