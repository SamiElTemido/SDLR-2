						</stimulus_data>
						<signals>
							<signal path="/BinaryToDecimal/STR"/>
						</signals>
					</stimulus>
					<stimulus name="DIN" strength="default" enabled="true" type="value">
						<stimulus_data>
							<value value="0000010001010111"/>
